<div class="row">
  <div class="span2">
    <p id="notice"><%= notice %></p>
  </div>
  <div class="span8">

    <fieldset>
      <div class="well">
        <h3>
          <%= @contact.name %>
        </h3>
        <hr>
        <h5>
          <p>
            <%= "Email: #{@contact.email }" %>
          </p>
          <p>
            <%= "Date of Birth: #{@contact.birthdate}" %>
          </p>
          <p>
            <%= "Phone number: #{@contact.phone_number}" %>
          </p>
        </h5>
      </div>
      <div class="well">
        <%= link_to 'Edit', edit_contact_path(@contact) %> |
        <%= link_to 'Back', contacts_path %>
      </div>
    </fieldset>

  </div>

</div>

